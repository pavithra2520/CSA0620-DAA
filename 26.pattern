#include<stdio.h> 
int main() 
{ 
	int rows,i,j,k,num=10; 
	printf("Enter the Rows:"); 
	scanf("%d",&rows); 
    for(i=1;i<=rows;i++){  
        for(j=i;j<=rows-1;j++){  
            printf(" ");   
        }  
        for(k=1;k<=i;k++){  
            printf("%d  ",k);  
        }    
        printf("\n");  
    }     
	return 0; 
}
